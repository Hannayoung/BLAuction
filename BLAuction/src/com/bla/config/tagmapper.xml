<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bla.mapper.TagMapper">

	<select id="select" parameterType="Integer" resultType="tag">
		SELECT * FROM TAG WHERE TAG_ID = #{auct_id}
	</select>
	
	<insert id="insert" parameterType="tag">
		INSERT INTO TAG VALUES (SEQ_TAG.NEXTVAL,#{tag_name},#{auct_id})
	</insert>
	
	<update id="update" parameterType="tag">
		 UPDATE TAG SET TAG_NAME = #{tag_name} WHERE TAG_ID = #{tag_id}
	</update>
	
	<delete id="delete" parameterType="Integer">
		DELETE * FROM TAG WHERE TAG_ID = #{tag_id}
	</delete>

</mapper>
